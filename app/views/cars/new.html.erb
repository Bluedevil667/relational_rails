<h1>New Car</h1>

<%= form_with(model: @car, url: cars_path, method: :post) do |form| %>
  <div>
    <%= form.label :make %>
    <%= form.text_field :make %>
  </div>

  <div>
    <%= form.label :model %>
    <%= form.text_field :model %>
  </div>

  <div>
    <%= form.label :year %>
    <%= form.text_field :year %>
  </div>

  <div>
    <%= form.submit "Create Car" %>
  </div>
<% end %>

<%= link_to "Back to Cars", cars_path %>
