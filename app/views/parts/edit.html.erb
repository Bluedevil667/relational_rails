<%= form_for @part do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
 
  <%= f.label :part_number %>
  <%= f.text_field :part_number %>
 
  <%= f.label :manufacturer %>
  <%= f.text_field :manufacturer %>
 
  <%= f.label :price %>
  <%= f.number_field :price %>
 
  <%= f.label :description %>
  <%= f.text_area :description %>
 
  <%= f.submit "Update Part" %>
<% end %>